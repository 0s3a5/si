  <section id="Hollow-Knigth">
        <div class="contenido-principal">
            <div class="cuadro-productos" id="cuadro-productos">
<div class="container my-5">
  <div class="row">
    {{#each lista}}
    <div class="col-sm-6 col-md-4 col-lg-3 mb-4">
      <div class="tarjeta-producto">
          <div class="tarjeta-contenido">
            <div class="frontal">
              <img src="{{{this.link_imagen}}}" alt="Producto" class="img-fluid">
              <div class="nombre">{{this.nombre}}</div>
            </div>
            <div class="trasera">
              <div class="descripcion">
                Precio: ${{this.precio}} USD<br>{{this.datof}}<br>{{this.datos}}
              </div>
            <a href="pantallaarma?paginaid={{this.id}}"> <!-- Añadir enlace aquí -->
              <button class="btn btn-primary mt-2">Comprar</button>
            </a>
            </div>
          </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>



    
