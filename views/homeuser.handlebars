 <body class="wallpaper">
  
    <div id="miDiv"></div>
    <script> //compara ancho y alto de la pantalla
        let dvh = window.innerHeight;
        let dvw = window.innerWidth;
        if (dvh > dvw) {
            document.getElementById('miDiv').style.height = '27%';
        } else {
            document.getElementById('miDiv').style.height = '20%';
        }
    </script>
   
    
   <div id="carouselExampleIndicators" class="carousel slide" style="width: 80%; margin: 0 auto;">
